<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="444.0" prefWidth="555.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PidocorreoController">
   <children>
      <Label text="Recuperar Contraseña">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label text="Verifique en su bandeja de entrada y sigua el vínculo">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label text="(Puede ser que el mismo haya ingresado en su bandeja de Span)">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="primaryButton" onAction="#switchToPrimary" text="Ok" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
